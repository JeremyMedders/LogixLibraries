<Routines>
<Routine Name="Logic" Type="ST">
<STContent>
<Line Number="0">
<![CDATA[]]>
</Line>
<Line Number="1">
<![CDATA[/*]]>
</Line>
<Line Number="2">
<![CDATA[	AO_Math_LowPassFilter]]>
</Line>
<Line Number="3">
<![CDATA[*/]]>
</Line>
<Line Number="4">
<![CDATA[]]>
</Line>
<Line Number="5">
<![CDATA[GSV(WallClockTime, , CurrentValue, DTNow);]]>
</Line>
<Line Number="6">
<![CDATA[DTDelta := DTNow - DTPrev;]]>
</Line>
<Line Number="7">
<![CDATA[]]>
</Line>
<Line Number="8">
<![CDATA[if (DTPrev > 0 & DTDelta > 0) then]]>
</Line>
<Line Number="9">
<![CDATA[	if (0.0 < Cfg_TimeConst & Cfg_TimeConst <= 1.0) then]]>
</Line>
<Line Number="10">
<![CDATA[		Alpha := e ** (DTDelta / (Cfg_TimeConst * -1.0E6));]]>
</Line>
<Line Number="11">
<![CDATA[	else]]>
</Line>
<Line Number="12">
<![CDATA[		Alpha := 0.0;]]>
</Line>
<Line Number="13">
<![CDATA[	end_if;]]>
</Line>
<Line Number="14">
<![CDATA[]]>
</Line>
<Line Number="15">
<![CDATA[	Val := Prev + (1.0 - Alpha) * (Inp_PV - Prev);]]>
</Line>
<Line Number="16">
<![CDATA[end_if;]]>
</Line>
<Line Number="17">
<![CDATA[]]>
</Line>
<Line Number="18">
<![CDATA[Prev := Val;]]>
</Line>
<Line Number="19">
<![CDATA[DTPrev := DTNow;]]>
</Line>
<Line Number="20">
<![CDATA[]]>
</Line>
</STContent>
</Routine>
<Routine Name="Prescan" Type="ST">
