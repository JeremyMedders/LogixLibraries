<Routines>
<Routine Name="Logic" Type="ST">
<STContent>
<Line Number="0">
<![CDATA[]]>
</Line>
<Line Number="1">
<![CDATA[/*]]>
</Line>
<Line Number="2">
<![CDATA[	T_DT_ShortTime]]>
</Line>
<Line Number="3">
<![CDATA[	]]>
</Line>
<Line Number="4">
<![CDATA[	10:18 PM]]>
</Line>
<Line Number="5">
<![CDATA[*/]]>
</Line>
<Line Number="6">
<![CDATA[]]>
</Line>
<Line Number="7">
<![CDATA[if (Inp.Hour >= 12) then]]>
</Line>
<Line Number="8">
<![CDATA[	StrAMPM := 'PM';]]>
</Line>
<Line Number="9">
<![CDATA[else]]>
</Line>
<Line Number="10">
<![CDATA[	StrAMPM := 'AM';]]>
</Line>
<Line Number="11">
<![CDATA[end_if;]]>
</Line>
<Line Number="12">
<![CDATA[]]>
</Line>
<Line Number="13">
<![CDATA[if (Inp.Hour > 12) then]]>
</Line>
<Line Number="14">
<![CDATA[	Inp.Hour := Inp.Hour - 12;]]>
</Line>
<Line Number="15">
<![CDATA[elsif (Inp.Hour = 0) then]]>
</Line>
<Line Number="16">
<![CDATA[	Inp.Hour := 12;]]>
</Line>
<Line Number="17">
<![CDATA[end_if;]]>
</Line>
<Line Number="18">
<![CDATA[]]>
</Line>
<Line Number="19">
<![CDATA[DTOS(Inp.Hour, StrHour12);]]>
</Line>
<Line Number="20">
<![CDATA[]]>
</Line>
<Line Number="21">
<![CDATA[CONCAT(Result, StrHour12, Result);]]>
</Line>
<Line Number="22">
<![CDATA[CONCAT(Result, StrColon, Result);]]>
</Line>
<Line Number="23">
<![CDATA[]]>
</Line>
<Line Number="24">
<![CDATA[if (Inp.Minute < 10) then]]>
</Line>
<Line Number="25">
<![CDATA[	CONCAT(Result, StrZero, Result);]]>
</Line>
<Line Number="26">
<![CDATA[end_if;]]>
</Line>
<Line Number="27">
<![CDATA[]]>
</Line>
<Line Number="28">
<![CDATA[DTOS(Inp.Minute, StrMinute);]]>
</Line>
<Line Number="29">
<![CDATA[]]>
</Line>
<Line Number="30">
<![CDATA[CONCAT(Result, StrMinute, Result);]]>
</Line>
<Line Number="31">
<![CDATA[CONCAT(Result, StrSpace, Result);]]>
</Line>
<Line Number="32">
<![CDATA[CONCAT(Result, StrAMPM, Result);]]>
</Line>
<Line Number="33">
<![CDATA[COP(Result, Val, 1);]]>
</Line>
<Line Number="34">
<![CDATA[DELETE(Result, Result.LEN, 1, Result);]]>
</Line>
<Line Number="35">
<![CDATA[]]>
</Line>
