</LocalTags>
<Routines>
<Routine Name="Logic" Type="ST">
<STContent>
<Line Number="0">
<![CDATA[]]>
</Line>
<Line Number="1">
<![CDATA[/*]]>
</Line>
<Line Number="2">
<![CDATA[	AO_Math_CxTANH]]>
</Line>
<Line Number="3">
<![CDATA[*/]]>
</Line>
<Line Number="4">
<![CDATA[]]>
</Line>
<Line Number="5">
<![CDATA[e := 2.71828182845905;]]>
</Line>
<Line Number="6">
<![CDATA[xi2 := 2.0 * A.I;]]>
</Line>
<Line Number="7">
<![CDATA[xr2 := 2.0 * A.R;]]>
</Line>
<Line Number="8">
<![CDATA[r := 1.0 / ((0.5 * (e ** xr2 + e ** -xr2)) + COS(xi2)); // COSH(x) = 0.5 * (e^x + e^-x)]]>
</Line>
<Line Number="9">
<![CDATA[]]>
</Line>
<Line Number="10">
<![CDATA[Val.R := r * (0.5 * (e ** xr2 - e ** -xr2)); // SINH(x) = 0.5 * (e^x - e^-x)]]>
</Line>
<Line Number="11">
<![CDATA[Val.I := r * SIN(xi2);]]>
</Line>
<Line Number="12">
<![CDATA[]]>
</Line>
</STContent>
</Routine>
</Routines>
