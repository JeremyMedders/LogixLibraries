<![CDATA[]]>
</Line>
<Line Number="7">
<![CDATA[if (PCmd_Reset & NOT OSR) then]]>
</Line>
<Line Number="8">
<![CDATA[	Val := 0.0;]]>
</Line>
<Line Number="9">
<![CDATA[	Val_Average := 0.0;]]>
</Line>
<Line Number="10">
<![CDATA[	Val_Acceleration := 0.0;]]>
</Line>
<Line Number="11">
<![CDATA[	Sum := 0.0;]]>
</Line>
<Line Number="12">
<![CDATA[	PVRising := 0;]]>
</Line>
<Line Number="13">
<![CDATA[	PVFalling := 0;]]>
</Line>
<Line Number="14">
<![CDATA[	PVSteady := 0;]]>
</Line>
<Line Number="15">
<![CDATA[	ROCRising := 0;]]>
</Line>
<Line Number="16">
<![CDATA[	ROCFalling := 0;]]>
</Line>
<Line Number="17">
<![CDATA[	COP(Inp_PV, Samples[0], Size); // assume a stopped starting point (not necessarily zero!)]]>
</Line>
<Line Number="18">
<![CDATA[	i0 := 0;]]>
</Line>
<Line Number="19">
<![CDATA[end_if;]]>
</Line>
<Line Number="20">
<![CDATA[OSR := PCmd_Reset;]]>
</Line>
<Line Number="21">
<![CDATA[]]>
</Line>
<Line Number="22">
<![CDATA[if (Cfg_PVDeadband < 0.0) then]]>
</Line>
<Line Number="23">
<![CDATA[	Cfg_PVDeadband := -Cfg_PVDeadband;]]>
</Line>
<Line Number="24">
<![CDATA[end_if;]]>
</Line>
<Line Number="25">
<![CDATA[]]>
</Line>
<Line Number="26">
<![CDATA[if (Cfg_ROCDeadband < 0.0) then]]>
</Line>
<Line Number="27">
<![CDATA[	Cfg_ROCDeadband := -Cfg_ROCDeadband;]]>
</Line>
<Line Number="28">
<![CDATA[end_if;]]>
</Line>
<Line Number="29">
<![CDATA[]]>
</Line>
<Line Number="30">
<![CDATA[if (Cfg_Interval > 0.0) then]]>
</Line>
<Line Number="31">
<![CDATA[	DeltaT := Cfg_Interval;]]>
</Line>
<Line Number="32">
<![CDATA[	//tDeltaTInv := 1.0 / tDeltaT;]]>
</Line>
<Line Number="33">
<![CDATA[else]]>
</Line>
<Line Number="34">
<![CDATA[	DeltaT := 1.0;]]>
</Line>
<Line Number="35">
<![CDATA[	Cfg_Interval := DeltaT;]]>
</Line>
<Line Number="36">
<![CDATA[	//tDeltaTInv := 1.0 / tDeltaT;]]>
</Line>
<Line Number="37">
<![CDATA[end_if;]]>
</Line>
<Line Number="38">
<![CDATA[]]>
</Line>
<Line Number="39">
<![CDATA[DeltaTInv := 1.0 / DeltaT;]]>
</Line>
<Line Number="40">
<![CDATA[]]>
</Line>
<Line Number="41">
<![CDATA[PG.Inp_Interval := Cfg_Interval * 1000;]]>
</Line>
<Line Number="42">
<![CDATA[PG.Inp_Enable := Inp_Enable & NOT PCmd_Reset;]]>
</Line>
<Line Number="43">
<![CDATA[AO_RTC_PulseGen(PG);]]>
</Line>
<Line Number="44">
<![CDATA[]]>
</Line>
<Line Number="45">
<![CDATA[if (PG.Out) then]]>
</Line>
<Line Number="46">
<![CDATA[	if (i0 >= Size) then]]>
</Line>
<Line Number="47">
<![CDATA[		i0 := 0;]]>
</Line>
<Line Number="48">
<![CDATA[	end_if;]]>
</Line>
<Line Number="49">
<![CDATA[	]]>
</Line>
<Line Number="50">
<![CDATA[	// record newest position and set trailing indices for velocity and acceleration calcs]]>
</Line>
<Line Number="51">
<![CDATA[	Samples[i0] := Inp_PV;]]>
</Line>
<Line Number="52">
<![CDATA[	i1 := (Size - (2 - (i0 + 1))) MOD Size; // "i0 - 1"]]>
</Line>
<Line Number="53">
<![CDATA[	i2 := (Size - (3 - (i0 + 1))) MOD Size; // "i0 - 2"]]>
</Line>
<Line Number="54">
<![CDATA[	]]>
</Line>
<Line Number="55">
<![CDATA[	// instantaneous velocity and acceleration]]>
</Line>
<Line Number="56">
<![CDATA[	Val := (Samples[i0] - Samples[i1]) / DeltaT;]]>
</Line>
<Line Number="57">
<![CDATA[	Val_Acceleration := (Samples[i0] - 2 * Samples[i1] + Samples[i2]) / (DeltaT * DeltaT);]]>
</Line>
<Line Number="58">
<![CDATA[	]]>
</Line>
<Line Number="59">
<![CDATA[	// average of recent velocities]]>
</Line>
<Line Number="60">
<![CDATA[	if (0 < Cfg_Depth & Cfg_Depth <= Size) then]]>
</Line>
<Line Number="61">
<![CDATA[		Depth := Cfg_Depth;]]>
</Line>
<Line Number="62">
<![CDATA[	else]]>
</Line>
<Line Number="63">
<![CDATA[		Depth := Size;]]>
</Line>
<Line Number="64">
<![CDATA[		Cfg_Depth := Depth;]]>
</Line>
<Line Number="65">
<![CDATA[	end_if;]]>
</Line>
<Line Number="66">
<![CDATA[	]]>
</Line>
<Line Number="67">
<![CDATA[	Val_Period := DeltaT * Depth;]]>
</Line>
<Line Number="68">
<![CDATA[	]]>
</Line>
<Line Number="69">
<![CDATA[	a1 := i0;]]>
</Line>
<Line Number="70">
<![CDATA[	a2 := (Size - (2 - (a1 + 1))) MOD Size; // "a - 1"]]>
</Line>
<Line Number="71">
<![CDATA[	Sum := 0.0;]]>
</Line>
<Line Number="72">
<![CDATA[	]]>
</Line>
<Line Number="73">
<![CDATA[	i:= 0;]]>
</Line>
<Line Number="74">
<![CDATA[	while i < Depth do]]>
</Line>
<Line Number="75">
<![CDATA[		Sum := Sum + (Samples[a1] - Samples[a2]) / DeltaT;]]>
</Line>
<Line Number="76">
<![CDATA[		a1 := a1 - 1;]]>
</Line>
<Line Number="77">
<![CDATA[		a2 := a2 - 1;]]>
</Line>
<Line Number="78">
<![CDATA[		if (a1 < 0) then a1 := Size - 1; end_if;]]>
</Line>
<Line Number="79">
<![CDATA[		if (a2 < 0) then a2 := Size - 1; end_if;]]>
</Line>
<Line Number="80">
<![CDATA[		i := i + 1;]]>
</Line>
<Line Number="81">
<![CDATA[	end_while;]]>
</Line>
<Line Number="82">
<![CDATA[	]]>
</Line>
<Line Number="83">
<![CDATA[	//for i := 0 to Depth - 1 by 1 do]]>
</Line>
<Line Number="84">
<![CDATA[	//	tSum := Sum + (Samples[a1] - Samples[a2]) / DeltaT;]]>
</Line>
<Line Number="85">
<![CDATA[	//	a1 := a1 - 1;]]>
</Line>
<Line Number="86">
<![CDATA[	//	a2 := a2 - 1;]]>
</Line>
<Line Number="87">
<![CDATA[	//	if (a1 < 0) then a1 := Size - 1; end_if;]]>
</Line>
<Line Number="88">
<![CDATA[	//	if (a2 < 0) then a2 := Size - 1; end_if;]]>
</Line>
<Line Number="89">
<![CDATA[	//end_for;]]>
</Line>
<Line Number="90">
<![CDATA[	]]>
</Line>
<Line Number="91">
<![CDATA[	Val_Average := Sum / Depth;]]>
</Line>
<Line Number="92">
<![CDATA[	]]>
</Line>
<Line Number="93">
<![CDATA[	// delta PV]]>
</Line>
<Line Number="94">
<![CDATA[	PVRising := Inp_PV > PVLast + Cfg_PVDeadband;]]>
</Line>
<Line Number="95">
<![CDATA[	PVFalling := Inp_PV < PVLast - Cfg_PVDeadband;]]>
</Line>
<Line Number="96">
<![CDATA[	PVSteady := (PVLast - Cfg_PVDeadband <= Inp_PV) & (Inp_PV <= PVLast + Cfg_PVDeadband);]]>
</Line>
<Line Number="97">
<![CDATA[	PVLast := Inp_PV;]]>
</Line>
<Line Number="98">
<![CDATA[	]]>
</Line>
<Line Number="99">
<![CDATA[	// delta acceleration]]>
</Line>
<Line Number="100">
<![CDATA[	//tROCRising := (Val_Acceleration - Cfg_ROCDeadband) > tROCLast;]]>
</Line>
<Line Number="101">
<![CDATA[	//tROCFalling := (Val_Acceleration + Cfg_ROCDeadband) < tROCLast & Val_Acceleration <> 0.0;]]>
</Line>
<Line Number="102">
<![CDATA[	//tROCSteady := (-Cfg_ROCDeadband <= Val_Acceleration) & (Val_Acceleration <= Cfg_ROCDeadband);]]>
</Line>
<Line Number="103">
<![CDATA[	]]>
</Line>
<Line Number="104">
<![CDATA[	ROCRising := Val_Acceleration > 0.0;]]>
</Line>
<Line Number="105">
<![CDATA[	ROCFalling := Val_Acceleration < 0.0;]]>
</Line>
<Line Number="106">
<![CDATA[	ROCSteady := Val_Acceleration = 0.0;]]>
</Line>
<Line Number="107">
<![CDATA[	]]>
</Line>
<Line Number="108">
<![CDATA[	i0 := i0 + 1;]]>
</Line>
<Line Number="109">
<![CDATA[end_if;]]>
</Line>
<Line Number="110">
<![CDATA[]]>
</Line>
<Line Number="111">
<![CDATA[Sts_Enabled := Inp_Enable;]]>
</Line>
<Line Number="112">
<![CDATA[Sts_PVIncreasing := PVRising;]]>
</Line>
<Line Number="113">
<![CDATA[Sts_PVDecreasing := PVFalling;]]>
</Line>
<Line Number="114">
<![CDATA[Sts_PVSteady := PVSteady;]]>
</Line>
<Line Number="115">
<![CDATA[Sts_PVAccelerating := ROCRising;]]>
</Line>
<Line Number="116">
<![CDATA[Sts_PVDecelerating := ROCFalling;]]>
</Line>
