<Routines>
<Routine Name="Logic" Type="ST">
<STContent>
<Line Number="0">
<![CDATA[]]>
</Line>
<Line Number="1">
<![CDATA[/*]]>
</Line>
<Line Number="2">
<![CDATA[	AO_OEE_Availability]]>
</Line>
<Line Number="3">
<![CDATA[*/]]>
</Line>
<Line Number="4">
<![CDATA[]]>
</Line>
<Line Number="5">
<![CDATA[Sts_Active := Inp_Active & NOT Inp_Down;]]>
</Line>
<Line Number="6">
<![CDATA[Sts_Idle := Inp_Idle & NOT (Inp_Active OR Inp_Down);]]>
</Line>
<Line Number="7">
<![CDATA[Sts_Down := Inp_Down;]]>
</Line>
<Line Number="8">
<![CDATA[]]>
</Line>
<Line Number="9">
<![CDATA[/*]]>
</Line>
<Line Number="10">
<![CDATA[	Command Map]]>
</Line>
<Line Number="11">
<![CDATA[	• Cmd.0: Clear starts]]>
</Line>
<Line Number="12">
<![CDATA[	• Cmd.1: Clear max]]>
</Line>
<Line Number="13">
<![CDATA[	• Cmd.2: Clear total]]>
</Line>
<Line Number="14">
<![CDATA[	• Cmd.3: Clear idle]]>
</Line>
<Line Number="15">
<![CDATA[	• Cmd.4: Clear down]]>
</Line>
<Line Number="16">
<![CDATA[	• Cmd.5: Clear all]]>
</Line>
<Line Number="17">
<![CDATA[*/]]>
</Line>
<Line Number="18">
<![CDATA[]]>
</Line>
<Line Number="19">
<![CDATA[Cmd := PCmd OR MCmd;]]>
</Line>
<Line Number="20">
<![CDATA[]]>
</Line>
<Line Number="21">
<![CDATA[if ((Cmd.0 OR Cmd.5) & NOT OSR.0) then]]>
</Line>
<Line Number="22">
<![CDATA[	Data.Starts := 0;]]>
</Line>
<Line Number="23">
<![CDATA[end_if;]]>
</Line>
<Line Number="24">
<![CDATA[OSR.0 := Cmd.0 OR Cmd.5;]]>
</Line>
<Line Number="25">
<![CDATA[]]>
</Line>
<Line Number="26">
<![CDATA[if ((NOT Sts_Active OR Cmd.5) & NOT OSR.1) then]]>
</Line>
<Line Number="27">
<![CDATA[	AO_OEE_ResetT(ResetT, Data.RunTime);]]>
</Line>
<Line Number="28">
<![CDATA[end_if;]]>
</Line>
<Line Number="29">
<![CDATA[OSR.1 := NOT Sts_Active OR Cmd.5;]]>
</Line>
<Line Number="30">
<![CDATA[]]>
</Line>
<Line Number="31">
<![CDATA[if ((Cmd.1 OR Cmd.5) & NOT OSR.2) then]]>
</Line>
<Line Number="32">
<![CDATA[	AO_OEE_ResetT(ResetT, Data.MaxRunTime);]]>
</Line>
<Line Number="33">
<![CDATA[end_if;]]>
</Line>
<Line Number="34">
<![CDATA[OSR.2 := Cmd.1 OR Cmd.5;]]>
</Line>
<Line Number="35">
<![CDATA[]]>
</Line>
<Line Number="36">
<![CDATA[if ((Cmd.2 OR Cmd.5) & NOT OSR.3) then]]>
</Line>
<Line Number="37">
<![CDATA[	AO_OEE_ResetT(ResetT, Data.TotalRunTime);]]>
</Line>
<Line Number="38">
<![CDATA[end_if;]]>
</Line>
<Line Number="39">
<![CDATA[OSR.3 := Cmd.2 OR Cmd.5;]]>
</Line>
<Line Number="40">
<![CDATA[]]>
</Line>
<Line Number="41">
<![CDATA[if ((Cmd.3 OR Cmd.5) & NOT OSR.4) then]]>
</Line>
<Line Number="42">
<![CDATA[	AO_OEE_ResetT(ResetT, Data.IdleTime);]]>
</Line>
<Line Number="43">
<![CDATA[end_if;]]>
</Line>
<Line Number="44">
<![CDATA[OSR.4 := Cmd.3 OR Cmd.5;]]>
</Line>
<Line Number="45">
<![CDATA[]]>
</Line>
<Line Number="46">
<![CDATA[if ((Cmd.4 OR Cmd.5) & NOT OSR.5) then]]>
</Line>
<Line Number="47">
<![CDATA[	AO_OEE_ResetT(ResetT, Data.DownTime);]]>
</Line>
<Line Number="48">
<![CDATA[end_if;]]>
</Line>
<Line Number="49">
<![CDATA[OSR.5 := Cmd.4 OR Cmd.5;]]>
</Line>
<Line Number="50">
<![CDATA[]]>
</Line>
<Line Number="51">
<![CDATA[/*]]>
</Line>
<Line Number="52">
<![CDATA[	Increment starts]]>
</Line>
<Line Number="53">
<![CDATA[*/]]>
</Line>
<Line Number="54">
<![CDATA[if (Sts_Active & NOT OSR.7) then]]>
</Line>
<Line Number="55">
<![CDATA[	Data.Starts := Data.Starts + 1;]]>
</Line>
<Line Number="56">
<![CDATA[end_if;]]>
</Line>
<Line Number="57">
<![CDATA[OSR.7 := Sts_Active;]]>
</Line>
<Line Number="58">
<![CDATA[]]>
</Line>
<Line Number="59">
<![CDATA[PG.Inp_Interval := 1000;]]>
</Line>
<Line Number="60">
<![CDATA[PG.Inp_Enable := (Sts <> 0);]]>
</Line>
<Line Number="61">
<![CDATA[AO_RTC_PulseGen(PG);]]>
</Line>
<Line Number="62">
<![CDATA[]]>
</Line>
<Line Number="63">
<![CDATA[if (PG.Out) then]]>
</Line>
<Line Number="64">
<![CDATA[	if (Sts_Active) then]]>
</Line>
<Line Number="65">
<![CDATA[		AO_OEE_IncT(IncT, Data.RunTime);]]>
</Line>
<Line Number="66">
<![CDATA[		AO_OEE_IncT(IncT, Data.TotalRunTime);]]>
</Line>
<Line Number="67">
<![CDATA[		]]>
</Line>
<Line Number="68">
<![CDATA[		if (Data.RunTime.Hours = Data.MaxRunTime.Hours) then]]>
</Line>
<Line Number="69">
<![CDATA[			if (Data.RunTime.Minutes = Data.MaxRunTime.Minutes) then]]>
</Line>
<Line Number="70">
<![CDATA[				if (Data.RunTime.Seconds > Data.MaxRunTime.Seconds) then]]>
</Line>
<Line Number="71">
<![CDATA[						Data.MaxRunTime.Seconds := Data.RunTime.Seconds;]]>
</Line>
<Line Number="72">
<![CDATA[				end_if;]]>
</Line>
<Line Number="73">
<![CDATA[			elsif (Data.RunTime.Minutes > Data.MaxRunTime.Minutes) then]]>
</Line>
<Line Number="74">
<![CDATA[					Data.MaxRunTime.Minutes := Data.RunTime.Minutes;]]>
</Line>
<Line Number="75">
<![CDATA[					Data.MaxRunTime.Seconds := 0;]]>
</Line>
<Line Number="76">
<![CDATA[			end_if;]]>
</Line>
<Line Number="77">
<![CDATA[		elsif (Data.RunTime.Hours > Data.MaxRunTime.Hours) then]]>
</Line>
<Line Number="78">
<![CDATA[			Data.MaxRunTime.Hours := Data.RunTime.Hours;]]>
</Line>
<Line Number="79">
<![CDATA[			Data.MaxRunTime.Minutes := 0;]]>
</Line>
<Line Number="80">
<![CDATA[			Data.MaxRunTime.Seconds := 0;]]>
</Line>
<Line Number="81">
<![CDATA[		end_if;]]>
</Line>
<Line Number="82">
<![CDATA[		]]>
</Line>
<Line Number="83">
<![CDATA[	elsif (Sts_Idle) then]]>
</Line>
<Line Number="84">
<![CDATA[		AO_OEE_IncT(IncT, Data.IdleTime);]]>
</Line>
<Line Number="85">
<![CDATA[		]]>
</Line>
<Line Number="86">
<![CDATA[	elsif (Sts_Down) then]]>
</Line>
<Line Number="87">
<![CDATA[		AO_OEE_IncT(IncT, Data.DownTime);]]>
</Line>
<Line Number="88">
<![CDATA[	end_if;]]>
</Line>
<Line Number="89">
<![CDATA[end_if;]]>
</Line>
<Line Number="90">
<![CDATA[]]>
</Line>
</STContent>
</Routine>
