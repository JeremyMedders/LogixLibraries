<Routines>
<Routine Name="Logic" Type="ST">
<STContent>
<Line Number="0">
<![CDATA[]]>
</Line>
<Line Number="1">
<![CDATA[/*	]]>
</Line>
<Line Number="2">
<![CDATA[	Num_DINTtoHex]]>
</Line>
<Line Number="3">
<![CDATA[*/]]>
</Line>
<Line Number="4">
<![CDATA[]]>
</Line>
<Line Number="5">
<![CDATA[COP(Inp, Bits[0], 4);]]>
</Line>
<Line Number="6">
<![CDATA[Str.LEN := 8;]]>
</Line>
<Line Number="7">
<![CDATA[]]>
</Line>
<Line Number="8">
<![CDATA[for i := 0 to 3 by 1 do]]>
</Line>
<Line Number="9">
<![CDATA[	// byte[i], first character]]>
</Line>
<Line Number="10">
<![CDATA[	Byte := (Bits[i] & 16#F0) / 16;]]>
</Line>
<Line Number="11">
<![CDATA[	if (Byte < 10) then]]>
</Line>
<Line Number="12">
<![CDATA[		Byte := Byte + 48;]]>
</Line>
<Line Number="13">
<![CDATA[	else]]>
</Line>
<Line Number="14">
<![CDATA[		Byte := Byte + 55;]]>
</Line>
<Line Number="15">
<![CDATA[	end_if;]]>
</Line>
<Line Number="16">
<![CDATA[	Str.DATA[(3 - i) * 2] := Byte;]]>
</Line>
<Line Number="17">
<![CDATA[	]]>
</Line>
<Line Number="18">
<![CDATA[	// byte[i], second character]]>
</Line>
<Line Number="19">
<![CDATA[	Byte := Bits[i] & 16#0F;]]>
</Line>
<Line Number="20">
<![CDATA[	if (Byte < 10) then]]>
</Line>
<Line Number="21">
<![CDATA[		Byte := Byte + 48;]]>
</Line>
<Line Number="22">
<![CDATA[	else]]>
</Line>
<Line Number="23">
<![CDATA[		Byte := Byte + 55;]]>
</Line>
<Line Number="24">
<![CDATA[	end_if;]]>
</Line>
<Line Number="25">
<![CDATA[	Str.DATA[(3 - i) * 2 + 1] := Byte;]]>
</Line>
<Line Number="26">
<![CDATA[end_for;]]>
</Line>
<Line Number="27">
<![CDATA[]]>
</Line>
<Line Number="28">
<![CDATA[COP(Str, Val, 1);]]>
</Line>
<Line Number="29">
<![CDATA[]]>
</Line>
</STContent>
