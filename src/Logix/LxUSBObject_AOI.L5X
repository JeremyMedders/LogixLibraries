<Routines>
<Routine Name="Logic" Type="ST">
<STContent>
<Line Number="0">
<![CDATA[]]>
</Line>
<Line Number="1">
<![CDATA[/*]]>
</Line>
<Line Number="2">
<![CDATA[	LxUSBObject]]>
</Line>
<Line Number="3">
<![CDATA[*/]]>
</Line>
<Line Number="4">
<![CDATA[]]>
</Line>
<Line Number="5">
<![CDATA[LxIO(LxIO, IO);]]>
</Line>
<Line Number="6">
<![CDATA[]]>
</Line>
<Line Number="7">
<![CDATA[PG.Inp_Enable := LxIO.Sts_Online;]]>
</Line>
<Line Number="8">
<![CDATA[LpPulse(PG);]]>
</Line>
<Line Number="9">
<![CDATA[]]>
</Line>
<Line Number="10">
<![CDATA[if (PG.Out & (Msg01.DN OR Msg01.ER OR NOT Msg01.EN)) then]]>
</Line>
<Line Number="11">
<![CDATA[	LxMsg(LxMsg, Msg01, 16#033A, 1, 0, 0, 1000000, IO);]]>
</Line>
<Line Number="12">
<![CDATA[	MSG(Msg01);]]>
</Line>
<Line Number="13">
<![CDATA[end_if;]]>
</Line>
<Line Number="14">
<![CDATA[]]>
</Line>
<Line Number="15">
<![CDATA[if (Msg01.DN OR Msg01.ER) then]]>
</Line>
<Line Number="16">
<![CDATA[	if (NOT Msg01.ER) then]]>
</Line>
<Line Number="17">
<![CDATA[		COP(MsgResp[0], Local, 1);]]>
</Line>
<Line Number="18">
<![CDATA[		Sts_PortConnected := Local.LogicalAddress <> 255;]]>
</Line>
<Line Number="19">
<![CDATA[	else]]>
</Line>
<Line Number="20">
<![CDATA[		Status := 0;]]>
</Line>
<Line Number="21">
<![CDATA[	end_if;]]>
</Line>
<Line Number="22">
<![CDATA[end_if;]]>
</Line>
<Line Number="23">
<![CDATA[]]>
</Line>
